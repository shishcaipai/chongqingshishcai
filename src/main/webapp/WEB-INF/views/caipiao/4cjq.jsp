            var $inner_syndicate_table = $('#inner_syndicate_table');
            // 清空原有数据
            var inner_syndicate_table = $inner_syndicate_table[0];
            var rows = inner_syndicate_table.rows;
            for (var i = rows.length - 1; i >= 0; i --) {                                                                                                                           
                if (!(/tit/i.test(rows[i].className))) {
                    inner_syndicate_table.deleteRow(rows[i].rowIndex);
