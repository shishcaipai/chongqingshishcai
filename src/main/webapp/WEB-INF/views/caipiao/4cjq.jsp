            }
            $('#inner_syndicate_table').append(obj.data).find('input').keydown(function(evt) {
                return numberKeyOnly(evt);
            }).end();
        }
    });
}
function ajaxFollowSyndicate(plan_id) {
    var buy_parts = parseInt($('#lottery_syndicate_' + plan_id).val(), 10);
    if (isNaN(buy_parts)) {
        $.showError('请正确输入要购买的份数！');
        return;
    }
    $.ajaxFollowSyndicate({
        plan_id: plan_id,
        parts: buy_parts,
        success: function(obj) {
